
#run main();

Vec2 :: struct {
    x, y: r64;
}

START_POS : Vec2 : { 1.0, 2.0 };
X :: 1;

main :: () {

    Y :: 2;

    print(Y);
    print(*Y);

    print(X);
    print(*X);

    print(START_POS);
    print(*START_POS);
    print_via_pointer(*START_POS);
    return 0;
}

print_via_pointer :: (v: *Vec2) {
    print("{ ", v.x, ", ", v.y, " }");
}
