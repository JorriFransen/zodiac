
#import "print.zc"

#run main();
main :: () {

    str := "abc, %i\n";
    printf(str.data, 55);
    println();

    print_func("Hello!");
    println();

    print_func("Hello!", "World", 42);
    println();

    args_arr: [3]Any;
    args_arr[0] = 11;
    args_arr[1] = 2.3;
    args_arr[2] = "xyz";

    args : []Any = args_arr;

    // print_func("Prepared args: ", args);

    return 0;

}

print_func :: (fmt: String, args: ..) -> void {
    println("fmt: \"", fmt, "\"");
    print("args: ");

    for i := 0; i < args.length; i += 1 {
        if i > 0 print(", ");
        print_any(args[i]);
    }

    println();
}

#foreign printf :: (cstr: *u8, args: ..) -> s32;

