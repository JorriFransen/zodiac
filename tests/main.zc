
#run main();

work :: (i: s64) { println(i); }

main :: () {

    defer println(5);
    println(0);

    for (i := 0; i < 3; i+=1) {

        defer println(4);
        println(1);

        {
            defer println(3);
            println(2);

            return 0;

            work(999);

        }


        defer println("x");
        println("y");
    }

    defer println("z");
    println("w");

    return 0;
}


