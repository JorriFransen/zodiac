#import "print.zc"

Vec2 :: struct {
    x, y: r64;
}

Day :: enum {
    Monday,
    Tuesday,
    Wednesday,
    Thursday,
    Friday,
    Saturday,
    Sunday,
}

#run main();
main :: () {

    x := 42;
    ax : Any = x;
    print_any(ax);
    println();

    print_any(x);
    println();

    x2 : u32 = 41;
    ax2 : Any = x2;
    print_any(ax2);
    println();

    print_any(x2);
    println();

    y := 4.2;
    ay : Any = y;
    print_any(ay);
    println();

    print_any(y);
    println();

    y2 : r64 = 4.3;
    ay2 : Any = y2;
    print_any(ay2);
    println();

    print_any(y2);
    println();

    b := true;
    ab : Any = b;
    print_any(ab);
    println();

    print_any(b);
    println();

    b2 := false;
    ab2 : Any = b2;
    print_any(ab2);
    println();

    print_any(b2);
    println();

    px := *x;
    apx : Any = px;
    print_any(apx);
    println();

    print_any(px);
    println();

    py := *y;
    apy : Any = py;
    print_any(apy);
    println();

    print_any(py);
    println();

    ppx := *px;
    appx : Any = ppx;
    print_any(appx);
    println();

    print_any(ppx);
    println();

    str := "Hello!";
    astr : Any = str;
    print_any(astr);
    println();

    print_any(str);
    println();

    vec : Vec2 = { 1.2, 5.4 };
    avec : Any = vec;
    print_any(avec);
    println();

    print_any(vec);
    println();

    d := Day.Tuesday;
    ad : Any = d;
    print_any(ad);
    println();

    print_any(d);
    println();

    d2 := Day.Friday;
    ad2 : Any = d2;
    print_any(ad2);
    println();

    print_any(d2);
    println();

    ints : [3]s64 = { 42, 43, -44 };
    aints : Any = ints;
    print_any(aints);
    println();

    print_any(ints);
    println();

    vecs : [4]Vec2 = { { 1, 2 }, { 3, 4 }, { 5, 6 }, { 7, 8 } };
    avecs : Any = vecs;
    print_any(avecs);
    println();

    print_any(vecs);
    println();

    slice : []s64 = { 11, 22, 33 };
    aslice : Any = slice;
    print_any(aslice);
    println();

    print_any(slice);
    println();

    slice2 : []Vec2 = vecs;
    aslice2 : Any = slice2;
    print_any(aslice2);
    println();

    print_any(slice2);
    println();

    binop_fn : (s64, s64) -> s64 = add_s64;
    abinop_fn : Any = binop_fn;
    print_any(abinop_fn);
    println();

    print_any(binop_fn);
    println();

    return 0;
}

add_s64 :: (a: s64, b: s64) -> s64 {
    return a + b;
}

