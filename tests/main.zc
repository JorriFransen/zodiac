
#import "print.zc"

#run main();

main :: () {

    a := string_copy("abc");
    b := string_copy("abc");

    println(a);
    println(b);

    println(string_eq(a, b));
    println(a == b);
    println(!string_eq(a, b));
    println(a != b);

    b[1] = 'B';

    println(a);
    println(b);

    println(string_eq(a, b));
    println(a == b);
    println(!string_eq(a, b));
    println(a != b);
}


