#import "print.zc"

#run main();
main :: () {

    // println("Hello world!");
    // println(1);

    // println(cstrlen("abc"));
    // println(strlen("abc"));

    println(cstrlen("abc"));

    return 0;

}

cstrlen :: (cstr: *u8) -> s64 {
    slice : []u8;
    slice.data = cstr;

    length := 0;
    while slice[length] != 0 {
        length += 1;
    }

    return length;
}

strlen :: (str: String) -> s64 {
    return str.length;
}
