
#run main();

Day :: enum {

    MONDAY;
    TUESDAY;
    WEDNESDAY,
    THURSDAY,
    FRIDAY;
    SATURDAY;
    SUNDAY;

}

Mode :: enum {
    READ  :: 11,
    WRITE :: 22;
    DRY_RUN;
}

Token_Kind :: enum {

    FIRST_NON_ASCII :: 256;
    TOK_NAME :: FIRST_NON_ASCII;
    TOK_INT;

}

main :: () {

    day : Day;
    println(day);
    day = Day.FRIDAY;
    println(day);
    println(Day.WEDNESDAY);

    return 0;
}
