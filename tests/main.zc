

add_s64 :: (a: s64, b: s64) -> s64 {
    return a + b;
}

S :: struct {
    binop_fn : (s64, s64) -> s64;
}

#run main();
main :: () {

    first : (s64, s64) -> s64;

    {
        add := add_s64;
        println(add(1, 2));
        lc := add;
        println(lc(1, 2));
    }

    {
        add : (s64, s64) -> s64 = add_s64;
        println(add(2, 3));
        first = add;
    }

    println(first(3, 4));

    s : S = { add_s64 };
    // s.binop_fn(4, 5);
    lc := s.binop_fn;
    println(lc(4, 5));

    return 0;
}
