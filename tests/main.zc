
#run main();


main :: () {

    ptr : *s64;
    println(ptr);

    if ptr println("ptr != null");
    else println("ptr == null");

    if !ptr println("ptr == null");
    else println("ptr != null");



    i : s64 = 42;
    ptr = *i;

    if ptr println("ptr != null");
    else println("ptr == null");

    if !ptr println("ptr == null");
    else println("ptr != null");

    b := false;
    println(b);

    if b println("b == true");
    else println("b == false");

    if !b println("b == false");
    else println("b == true");

    b = true;

    if b println("b == true");
    else println("b == false");

    if !b println("b == false");
    else println("b == true");

    return 0;
}
