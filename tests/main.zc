
#run main();

global_arr : [3]s64 = { 1, 2, 3 };
c_global_arr : [3]s64 = { 11, 22, 33 };

main :: () {
    arr1 : [2]s64 = { 1, 2 };
    carr : [2]s64 : { 11, 22 };
    print_slice(arr1);
    print_slice(carr);
    print_slice(global_arr);
    print_slice(c_global_arr);
    print_slice({ 2, 1 });
    return 0;
}

print_slice :: (s: []s64) {
    println(s);
}
