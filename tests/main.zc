
#import "print.zc"

#run main();
main :: () {

    // print_func("Hello!");
    // print_func("Hello!", "World");

    args : [2]Any;
    args[0] = 42;
    args[1] = 4.2;

    print_func("Numbers: ", args);
    println();

    return 0;

}

print_func :: (fmt: String, args: ..) -> void {
    println("fmt: \"", fmt, "\"");
    print("args: ");

    for i := 0; i < args.length; i += 1 {
        if i > 0 print(", ");
        print_any(args[i]);
    }
}

